{
  "name": "targito/targito-api-bundle",
  "type": "symfony-bundle",
  "description": "Targito API implementation for Symfony",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": "^7.2|^8.0",
    "symfony/framework-bundle": "^4.4|^5.0",
    "symfony/yaml": "^4.4|^5.0",
    "targito/targito-api": "^1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Targito\\Bundle\\Api\\": "src/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpstan/phpstan": "^0.12.18"
  },
  "scripts": {
    "fixer": "php-cs-fixer fix -v",
    "phpstan": "phpstan analyse --level max src"
  },
  "config": {
    "platform": {
      "php": "7.2.22"
    }
  }
}
